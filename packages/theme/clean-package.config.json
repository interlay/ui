{
  "remove": ["devDependencies"],
  "replace": {
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      },
      "./dist/*.css": {
        "import": "./dist/*.css",
        "require": "./dist/*.css"
      },
      "./package.json": "./package.json"
    }
  }
}
